        "libs/administration/shared/ui/src/index.ts"
      ],
      "@frontend/administration/shared/util": [
        "libs/administration/shared/util/src/index.ts"
      ],
      "@frontend/website/authenticate/feature-login": [
        "libs/website/authenticate/feature-login/src/index.ts"
      ],
