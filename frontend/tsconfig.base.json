        "libs/website/authenticate/data-access/src/index.ts"
      ],
      "@frontend/website/availability/feature-shell": [
        "libs/website/availability/feature-shell/src/index.ts"
      ],
      "@frontend/website/authenticate/feature-shell": [
        "libs/website/authenticate/feature-shell/src/index.ts"
      ],
      "@frontend/shared/data-access": ["libs/shared/data-access/src/index.ts"]
    }
